
{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.[a-z]{2,4})$",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}